# sudo works with aliases
alias sudo='sudo '

# sudi is sudo -i
alias sudi='sudo -i'

# netcat udp hostname port
alias nc-u='nc -u -zv'

# netcat tcp hostname port
alias nc-t='nc -t -zv'

# l is ls -alF
alias l='ls -alF'

# ll is ls -l
alias ll='ls -l'

# la is ls -la
alias la='ls -la'

# lat is ls -lat
alias lat='ls -lat'

# latr is ls -latr
alias latr='ls -latr'

# lt is ls -lt
alias lt='ls -lt'

# pacman list packages by time
alias paclog='expac --timefmt="%Y-%m-%d %T" "%l\t%n" | sort | less +G'

# cd to git
alias cdgit='cd ~/git/github.com/jakegiorlando'
